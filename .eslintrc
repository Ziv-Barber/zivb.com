{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "plugin:flowtype/recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard"
  ],
  "rules": {
    "no-unused-vars": 0
  },
  "parserOptions": {},
  "plugins": [
    "flowtype",
    "react"
  ],
  "env": {
    "jest": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "latest"
	}
  }
}
