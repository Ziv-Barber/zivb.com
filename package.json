{
  "name": "zivb.com",
  "version": "0.1.0",
  "homepage": "https://Ziv-Barber.github.io/zivb.com",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.8.3",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "clean": "npx rimraf build docs",
    "commit": "npx git-cz",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "esdoc": "npx esdoc",
    "flow": "npx flow",
    "lint": "better-npm-run lint",
    "prettify": "prettier --write",
    "prettify:all": "prettier --write \"{src,__tests__}/**/*.{jsx,js}\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "betterScripts": {
    "lint": {
      "command": "npx eslint \"**/*.{js,jsx}\"",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "devDependencies": {
    "better-npm-run": "^0.1.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-brand-plugin": "^1.0.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-integrate-manual-plugin": "^1.0.0",
    "esdoc-integrate-test-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-lint-plugin": "^1.0.2",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.89.0",
    "flow-typed": "^2.5.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.16.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3"
  }
}
